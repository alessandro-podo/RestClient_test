{
  "name": "alessandro_podo/restclient",
  "description": "A RestClient for Symfony",
  "minimum-stability": "stable",
  "license": "proprietary",
  "type": "symfony-bundle",
  "keywords": [
    "api",
    "rest",
    "restclient"
  ],
  "homepage": "https://coden.mitsandro.de",
  "authors": [
    {
      "name": "Alessandro Podo",
      "email": "coden@mitsandro.de"
    }
  ],
  "require": {
    "php": "8.*",
    "symfony/serializer": "^5.3",
    "symfony/var-exporter": "^5.3",
    "symfony/http-client": "^5.3",
    "phpunit/phpunit": "^9.5",
    "symfony/validator": "^5.3",
    "doctrine/annotations": "^1.13",
    "symfony/cache": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "RestClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RestClientTests\\": "Tests/"
    }
  },
  "require-dev": {
    "symfony/dependency-injection": "^5.3",
    "symfony/config": "^5.3",
    "symfony/http-kernel": "^5.3",
    "symfony/yaml": "^5.3",
    "phpstan/phpstan": "^0.12.98",
    "drew/debug-statements-fixers": "0.4"
  }
}
